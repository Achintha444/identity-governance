{
  "identity.recaptcha.site_key": [
    {
      "if": {
        "identity.recaptcha.enabled": true
      },
      "regex": ".+",
      "error_message": "Recaptcha site key is required."
    }
  ],

  "identity.recaptcha.secret_key": [
    {
      "if": {
        "identity.recaptcha.enabled": true
      },
      "regex": ".+",
      "error_message": "Recaptcha secret key is required."
    }
  ]
}
